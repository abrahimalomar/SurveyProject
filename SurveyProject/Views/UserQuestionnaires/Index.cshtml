@model IEnumerable<SurveyProject.Models.UserQuestionnaire>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table" id="example1">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.answer)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QutionId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Questionnaire)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.answer)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Questions.QuestionTitile)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Questionnaire.Titel)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.User.UserName)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
@section Scripts
    {
    <!--DataTable-->
    <script src="~/js/Datatable.js"></script>
    <script src="~/lib/datatables/js/jquery.dataTables.js"></script>

    <script src="~/lib/datatables/js/jquery.dataTables.min.js"></script>

}